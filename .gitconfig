[alias]
  last = log -1 HEAD
  unstage = reset HEAD --
[core]
  editor = nvim
[diff]
  tool = fugitive
[merge]
	tool = fugitive
[difftool]
  prompt = false
  trustExitCode = true
[mergetool "fugitive"]
	cmd = nvim -f -c "Gvdiffsplit!" "$MERGED"
